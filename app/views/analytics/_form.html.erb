<%= simple_form_for(@analytic) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :project_id %>
    <%= f.input :download_brochure_timestamp %>
    <%= f.input :callback_timestamp %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
