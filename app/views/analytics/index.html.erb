<p id="notice"><%= notice %></p>

<h1>Analytics</h1>

<table>
  <thead>
    <tr>
      <th>user ID</th>
      <th>Project</th>
      <th>Download brochure timestamp</th>
      <th>Callback timestamp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analytics.each do |analytic| %>
      <tr>
        <td><%= analytic.user_id %></td>
        <td><%= analytic.project_id %></td>
        <td><%= analytic.download_brochure_timestamp %></td>
        <td><%= analytic.callback_timestamp %></td>
        <td><%= link_to 'Show', analytic %></td>
        <td><%= link_to 'Edit', edit_analytic_path(analytic) %></td>
        <td><%= link_to 'Destroy', analytic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Analytic', new_analytic_path %>
